<?xml version="1.0" encoding="utf-8"?><!--定义一个带圆角,白色背景,绿色边框的矩形-->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle"
    android:useLevel="false">
    <!--圆角弧度-->
    <corners android:radius="@dimen/ridus_10"/>
    <!-- 实心 -->
    <solid android:color="@color/white"/>
    <!-- 描边 -->
    <stroke
        android:width="1dp"
        android:color="@color/title_blue_bg" />
    <size
        android:height="20dp"/>
</shape>

